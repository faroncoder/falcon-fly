NODE_PATH=/usr/local/bin/node;
#NWJS=/usr/local/bin/nwjs;
#ANDROID_HOME=/home/users/$USER/Android/Sdk;

NEWPATH="$( getconf PATH ):/sbin:/usr/sbin:/usr/local/bin:/usr/local/sbin:$JAVA_HOME/bin:$NODE_PATH"

CLEAN=`echo $NEWPATH | tr ':' '\n' | sort | uniq | tr '\n' ':'`

export PATH=$CLEAN



alias rm="trash-put"
alias rm1="rm"

