PATH=""
NODE_HOME=/usr/local/include/node
NPM_HOME=/usr/local/lib/node_modules/npm
JAVA_HOME=/usr/lib/jvm/java-8-oracle
JRE_HOME=/usr/lib/jvm/java-8-oracle/jre
XE=/usr
DA=bin
DF=sbin
LD=$XE/local


	GETPATH=( $XE/$DA /$DA /$DF $LD/lib/depot_tools $XE/$DF $LD/$DA  $LD/$DF $JAVA_HOME/$DA  $JRE_HOME/$DA $NODE_HOME $NPM_HOME/bin )
	SEMI=""
	for dir in "${GETPATH[@]}"; do
		NEWI="$SEMI$dir"
		CLEAN="$CLEAN$NEWI"
		SEMI=":"
	done
	export PATH=$CLEAN

alias rm="trash-put"
alias rm1="rm"

