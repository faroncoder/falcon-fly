getfiles=( "$( find $PWD -maxdepth 1 -name '*.mkv' -exec basename {} \; )" );
echo ${getfiles[@]}
for x in "${getfiles[@]}"; do ffmpeg --ss 00:01:00 -i $x -y -t 1 -vf scale=-1:256 -f image 2 -vframes 1 $x.png; ; done
for x in "${getfiles[@]}"; do ffmpeg --ss 00:01:00 -i $x -y -t 1 -vf scale=-1:256 -f image 2 -vframes 1 $x.png; done
for x in "${getfiles[@]}"; do ffmpeg -ss 00:01:00 -i $x -y -t 1 -vf scale=-1:256 -f image 2 -vframes 1 $x.png; done
for x in "${getfiles[@]}"; do ffmpeg -ss 00:00:50 -to 00:01:01 -i $x -y -c:v copy -c:a copy - | ffmpeg -ss 00:00:10 -i - -t 1 -vf scale=-1:256 -f image2 -vframes 1 $x.png; done
for x in "${getfiles[@]}"; do ffmpeg -ss 00:01:00 -i $x -y -c:v copy -c:a copy -t 1 -vf scale=-1:256 -vframes 1 -f image2 $x.png; done
for x in "${getfiles[@]}"; do ffmpeg -ss 00:01:00 -i $x -y -c:v libx264 -t 1 -vf scale=-1:256 -vframes 1 -an -f image2 $x.png; done
ls
for x in "${getfiles[@]}"; do ffmpeg -ss 00:01:00 -i $x -y -t 1 -vf scale=-1:256 -vframes 1 -f image2 $x.png; done
ls
ffopen 206.mkv.png 
ffplay 206.mkv.png 
exit
