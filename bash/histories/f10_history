LISTINGS=( $( find . -maxdepth 1 -type f -name '*.mp4' -exec basename {} \; ) );
for f in "${LISTINGS[@]}"; do mediainfo $f > a.txt;  while read line; do echo $line >> b.txt; done < a.txt; rm a.txt;  NEWTHIS="$( grep 'Encoding settings' b.txt | sed 's/Encoding\ settings\ ://g' )";  FARONINSERT="$( echo "\"Enconding settings\": { $( echo $NEWTHIS | tr ',' '|' | tr '/' ','  | perl -e 'use JSON; @in=grep(s/\n$//, <>); print encode_json(\@in)."\n";' | sed 's/\ ,\ /","/g' | sed 's/=/":"/g' ) } }" | sed 's/\[//g' | sed 's/\]//g' | sed 's/|/,/g' | sed 's/"Encoding\ settings\ :\ /"/g' )"; cat b.txt | sed '/^$/d' | sed 's/MPEG\ Audio/MPEG-audio/g' | sed 's/General/General" : {/g' | sed 's/Video/},\nVideo" : {/g' | sed 's/Audio/},\nAudio": {/g' > c.txt; echo "} }" >> c.txt; rm b.txt; cat c.txt | perl -e 'use JSON; @in=grep(s/\n$//, <>); print encode_json(\@in)."\n";' | sed 's/,"},","Audio\\": {","/},"Audio": {"/g' | sed 's/,"},","Video\\": {","/}, "Video": {"/g' | sed 's/\["General\\" : {","/{ "General": { "/g' | sed 's/ : /":"/g' | sed 's/","},","/"},"/g' | sed 's/"Video\\"":"{","/"Video":{"/g' | sed 's/","} }"]/"} }/g' > d.txt;  rm c.txt;  cat d.txt | sed 's/"} }/"}, "Encoding Settings": { FARONINSERT } }/g' | sed 's/FARONINSERT/$( echo $FARONINSERT)/g' > $f.json; rm d.txt; rename 's/.mp4.json/.json/g' $f.json; done
