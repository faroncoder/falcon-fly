curl --url http://ip-api.com/json | jq ."lat,lon"
curl --url http://ip-api.com/json | jq ."lat"."lon"
curl --url http://ip-api.com/json | jq ."lat" ."lon"
curl --url http://ip-api.com/json | jq .["lat","lon"]
curl --url http://ip-api.com/json | jq .[lat,lon]
curl --url http://ip-api.com/json | jq .lat,lon
curl --url http://ip-api.com/json | jq .lat
curl --url http://ip-api.com/json | jq .lat .lon
curl --url http://ip-api.com/json | jq .lat[] .lon[]
curl --url http://ip-api.com/json | jq .lat[],.lon[]
curl --url http://ip-api.com/json | jq lat[],.lon[]
curl --url http://ip-api.com/json | jq lat[] .lon[]
curl --url http://ip-api.com/json | jq .lon
curl --url http://ip-api.com/json | jq .lon && jq .lat
