loadScripts() {
		if ! shopt -oq posix; then
				if [ -f /usr/share/bash-completion/bash_completion ]; then
					. /usr/share/bash-completion/bash_completion
				elif [ -f /etc/bash_completions ]; then
					. /etc/bash_completion
				fi
				alias rm="trash-put"
		fi

}

loadEnvironment() {
loadScripts
DEFAULT="/home/faron/.bin:/usr/local/bin"
JAVA_HOME="/usr/local/jvm/jdk1.8.0_60"
JRE_HOME="/usr/local/jvm/jre1.8.0_60"
CLEANBINS="$PATH:$DEFAULT:$JAVA_HOME/bin:$JRE_HOME/bin"
CLEANUP=$( echo $CLEANBINS | tr ':' '\n' | sort | uniq | sort | tr '\n' ':' | sed 's/::/:/g' )
export PATH=$CLEANUP
}

######### FARONIZED-MENU FUNCTION
    cd()  {
	builtin cd "$@"
    	RET="$?"
 	find $PWD -maxdepth 1 -type d -empty -exec rm -r {} \; > /dev/null  2>&1
    	ls
    	return $RET
}

