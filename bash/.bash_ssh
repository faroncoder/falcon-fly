#!/bin/bash


ssh-engine() {
	echo "Initializing SSH agent..."
	chmod -R 0700 $HOME/.ssh
	echo "privizating $HOME/.ssh : done"
	ssh-agent;
	eval $( ssh-agent -s );
	find -L "$HOME/.ssh" -type f ! -type d -name 'id_*' ! -name '*.pub' -exec /usr/bin/ssh-add {} \;
}

ssh-check() {
	/home/faron/.bin/./ff.cpu.pidof ssh-agent
	# GETRESULT="$( /home/faron/.bin/./ff.cpu.pidof ssh-agent | cut -d\":\" -f2 )"
}

