ssh-engine() {
	echo "Initializing SSH agent..."
	chmod -R 0700 $HOME/.ssh
	echo "privizating $HOME/.ssh : done"
	ssh-agent -s
	eval $( ssh-agent -s );
	/usr/bin/find -L $HOME/.ssh -type f -name 'id_*' ! -name '*.pub' -exec /usr/bin/ssh-add {} \;
}

ssh-check() {
	/home/faron/.bin/ff.cpu.pidof ssh-agent
	GETRESULT=$( `ff.cpu.pidof ssh-agent | cut -d":" -f2` )
	ssh-engine
	echo -e $GETRESULT
}

